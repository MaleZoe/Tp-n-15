i DROP TABLE IF EXISTS medicamentos
ii CREATE TABLE medicamentos (
    codigo INT AUTO_INCREMENT,
    nombre VARCHAR(20),
    laboratorio VARCHAR(20),
    precio FLOAT,
    cantidad INT,
    PRIMARY KEY (codigo)
); 
iii DESCRIBE medicamentos;
iv INSERT INTO medicamentos (nombre, laboratorio, precio, cantidad) VALUES
    ('Sertal', 'Roche', 5.2, 100),
    ('Buscapina', 'Roche', 4.10, 200),
    ('Amoxidal 500', 'Bayer', 15.60, 100);
v SELECT codigo, nombre, laboratorio, precio, cantidad FROM medicamentos;
